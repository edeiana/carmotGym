#!/bin/bash -e

THIS_PATH="$( cd "$( dirname "${BASH_SOURCE[0]}" )" >/dev/null 2>&1 && pwd )" ;
CARMOT_HOME="${THIS_PATH}/.." ;

prefixString="### CARMOT Gym: overhead red bars ###" ;
echo "${prefixString}" ;

${CARMOT_HOME}/bin/carmot_fig7-10-11_baseline ;
${CARMOT_HOME}/bin/carmot_fig7_redbar ;
${CARMOT_HOME}/bin/carmot_fig10_redbar ;
${CARMOT_HOME}/bin/carmot_fig11_redbar ;

